<div id="main-wrapper">
	
	<div id="map-ui">
		<h2>Teams</h2>
		<div id="team-selector-div">
			<div class="form-check">
					<input onclick="set_team('black')" class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault0" checked>
				<label class="form-check-label" for="flexRadioDefault2">
					Not assigned yet
				</label>
			</div>
		
			<% for(color of team_colors){ %>
				<div class="color-form-check" >
					<input style="background-color: <%= color %>;" onclick="set_team('<%= color %>')" class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault1">
					<label class="form-check-label" for="flexRadioDefault1">
						Team <%= color %>
					</label>
				</div>
			<% }%>
		</div>
		
	</div>

	<div id="map">
	</div>
	<div id = chart-wrapper>
		<canvas id="elevations"></canvas>
	</div>
</div>