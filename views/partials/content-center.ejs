<div id="main-wrapper">
	
	<div id="map-ui">
		<h2>Teams</h2>
		<div id="team-selector-div">
			<div class="form-check">
					<input onclick="set_team('black')" class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault0" checked>
				<label class="form-check-label" for="flexRadioDefault2">
					Not assigned
				</label>
			</div>
		
			<% for(color of team_colors){ %>
				<div class="color-form-check" >
					<input onclick="set_team('<%= color %>')" style="background-color: <%= color %>;" class="form-check-input" type="radio" name="flexRadioDefault" id="flexRadioDefault<%= color %>">
					<label class="form-check-label" for="flexRadioDefault1">
						Team <%= color %>
					</label>
				</div>
			<% }%>
		</div>
		
	</div>

	<div id="map">
	</div>

	<!-- <div id="chart-wrapper" style="width: 100%;"> -->
		<!-- By default the elevations are hidden (nothing selected) -->
		<canvas id="elevations" style="display: none; width: 100%;"> </canvas>
	<!-- </div> -->
</div>